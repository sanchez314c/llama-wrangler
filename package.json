{
  "name": "llama-wrangler",
  "version": "1.0.0",
  "description": "Universal LLM Model Manager - Switch, Download, and Convert Models with Ease",
  "main": "src/main.js",
  "scripts": {
    "start": "electron .",
    "dev": "electron . --dev",
    "dist": "electron-builder --mac --win --linux",
    "dist:all": "electron-builder --mac --win --linux --publish=never",
    "dist:current": "electron-builder",
    "dist:mac": "electron-builder --mac",
    "dist:mac:all": "electron-builder --mac --x64 --arm64",
    "dist:mac:store": "electron-builder --mac --config.mac.target=mas",
    "dist:win": "electron-builder --win",
    "dist:win:all": "electron-builder --win --x64 --ia32 --arm64",
    "dist:win:msi": "electron-builder --win --config.win.target=msi",
    "dist:win:portable": "electron-builder --win --config.win.target=portable",
    "dist:linux": "electron-builder --linux",
    "dist:linux:all": "electron-builder --linux --x64 --arm64 --armv7l",
    "dist:linux:appimage": "electron-builder --linux --config.linux.target=AppImage",
    "dist:linux:deb": "electron-builder --linux --config.linux.target=deb",
    "dist:linux:rpm": "electron-builder --linux --config.linux.target=rpm",
    "dist:linux:snap": "electron-builder --linux --config.linux.target=snap",
    "dist:linux:tar": "electron-builder --linux --config.linux.target=tar.xz",
    "dist:maximum": "electron-builder --mac --win --linux --x64 --arm64 --ia32 --armv7l",
    "pack": "electron-builder --dir",
    "postinstall": "electron-builder install-app-deps && npm run security-check",
    "bloat-check": "./scripts/bloat-check.sh",
    "temp-clean": "./scripts/temp-cleanup.sh",
    "build-clean": "./scripts/compile-build-dist.sh --no-temp-clean --no-bloat-check",
    "build": "npm run dist",
    "build:mac": "npm run dist:mac",
    "build:win": "npm run dist:win",
    "build:linux": "npm run dist:linux",
    "build:all": "npm run dist",
    "clean": "rm -rf dist build out node_modules/.cache",
    "rebuild": "npm run clean && npm install && npm run build",
    "devtools": "electron . --dev --inspect",
    "test": "echo \"No tests specified\" && exit 0",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "lint:check": "eslint src --ext .js,.jsx,.ts,.tsx",
    "type-check": "tsc --noEmit",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx,json,css,md}",
    "format:check": "prettier --check src/**/*.{js,jsx,ts,tsx,json,css,md}",
    "security-check": "npm audit --audit-level=moderate",
    "deps-check": "ncu --checkUpgrades",
    "precommit": "npm run lint && npm run test && npm run security-check"
  },
  "keywords": [
    "llama",
    "llm",
    "ai",
    "model-manager",
    "huggingface",
    "ollama",
    "gguf"
  ],
  "author": {
    "name": "Jasonn Michaels",
    "email": "sanchez314c@jasonpaulmichaels.co"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/llamawrangler/llama-wrangler.git"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@types/node": "^24.9.2",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "electron": "^39.0.0",
    "electron-builder": "^26.0.12",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "prettier": "^3.6.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "electron-context-menu": "^4.1.1",
    "electron-store": "^11.0.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "vite": "^7.1.12"
  },
  "build": {
    "appId": "com.llamawrangler.app",
    "productName": "Llama Wrangler",
    "copyright": "Copyright Â© 2024 Llama Wrangler Team",
    "directories": {
      "output": "dist",
      "buildResources": "resources"
    },
    "files": [
      "src/**/*",
      "resources/**/*",
      "package.json",
      "!**/*.ts",
      "!**/*.map",
      "!**/*.md",
      "!**/test/**",
      "!**/tests/**",
      "!**/__tests__/**",
      "!**/spec/**",
      "!**/docs/**",
      "!**/README*",
      "!**/CHANGELOG*",
      "!**/LICENSE*",
      "!**/.git/**",
      "!**/.vscode/**",
      "!**/node_modules/.cache/**",
      "!**/build-temp/**",
      "!**/old_scripts/**",
      "!**/node_modules_old/**"
    ],
    "compression": "maximum",
    "extraResources": [
      {
        "from": "scripts/",
        "to": "scripts/",
        "filter": [
          "**/*"
        ]
      }
    ],
    "mac": {
      "category": "public.app-category.developer-tools",
      "sign": false,
      "identity": null,
      "icon": "resources/icons/icon.icns",
      "hardenedRuntime": false,
      "gatekeeperAssess": false,
      "target": [
        {
          "target": "dmg",
          "arch": [
            "x64",
            "arm64",
            "universal"
          ]
        },
        {
          "target": "zip",
          "arch": [
            "x64",
            "arm64",
            "universal"
          ]
        },
        {
          "target": "pkg",
          "arch": [
            "x64",
            "arm64",
            "universal"
          ]
        }
      ]
    },
    "mas": {
      "category": "public.app-category.developer-tools",
      "icon": "resources/icons/icon.icns",
      "hardenedRuntime": true,
      "entitlements": "resources/entitlements.mas.plist",
      "entitlementsInherit": "resources/entitlements.mas.inherit.plist",
      "target": {
        "target": "mas",
        "arch": [
          "x64",
          "arm64"
        ]
      }
    },
    "win": {
      "icon": "resources/icons/icon.ico",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32",
            "arm64"
          ]
        },
        {
          "target": "msi",
          "arch": [
            "x64",
            "ia32",
            "arm64"
          ]
        },
        {
          "target": "zip",
          "arch": [
            "x64",
            "ia32",
            "arm64"
          ]
        },
        {
          "target": "portable",
          "arch": [
            "x64",
            "ia32",
            "arm64"
          ]
        },
        {
          "target": "appx",
          "arch": [
            "x64",
            "ia32",
            "arm64"
          ]
        }
      ]
    },
    "linux": {
      "icon": "resources/icons",
      "category": "Utility",
      "target": [
        {
          "target": "AppImage",
          "arch": [
            "x64",
            "arm64",
            "armv7l"
          ]
        },
        {
          "target": "deb",
          "arch": [
            "x64",
            "arm64",
            "armv7l"
          ]
        },
        {
          "target": "rpm",
          "arch": [
            "x64",
            "arm64",
            "armv7l"
          ]
        },
        {
          "target": "snap",
          "arch": [
            "x64",
            "arm64",
            "armv7l"
          ]
        },
        {
          "target": "tar.xz",
          "arch": [
            "x64",
            "arm64",
            "armv7l"
          ]
        },
        {
          "target": "tar.gz",
          "arch": [
            "x64",
            "arm64",
            "armv7l"
          ]
        }
      ]
    },
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "nsis": {
      "oneClick": false,
      "installerIcon": "resources/icons/icon.ico",
      "uninstallerIcon": "resources/icons/icon.ico",
      "installerHeaderIcon": "resources/icons/icon.ico",
      "allowToChangeInstallationDirectory": true,
      "perMachine": false,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "Llama Wrangler"
    },
    "msi": {
      "oneClick": false,
      "perMachine": false,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "upgradeCode": "12345678-1234-5678-9012-123456789012"
    },
    "portable": {
      "requestExecutionLevel": "user"
    },
    "appx": {
      "applicationId": "LlamaWrangler",
      "backgroundColor": "#464646",
      "displayName": "Llama Wrangler",
      "identityName": "LlamaWranglerTeam.LlamaWrangler",
      "publisherDisplayName": "Llama Wrangler Team"
    },
    "deb": {
      "depends": [
        "libgtk-3-0",
        "libnotify4",
        "libnss3",
        "libxss1",
        "libxtst6",
        "xdg-utils",
        "libatspi2.0-0",
        "libuuid1",
        "libsecret-1-0"
      ]
    },
    "rpm": {
      "depends": [
        "gtk3",
        "libnotify",
        "nss",
        "libXScrnSaver",
        "libXtst",
        "xdg-utils",
        "at-spi2-core",
        "libuuid",
        "libsecret"
      ]
    },
    "snap": {
      "grade": "stable",
      "summary": "Universal LLM Model Manager"
    },
    "appImage": {},
    "publish": [
      {
        "provider": "github",
        "owner": "llamawrangler",
        "repo": "llama-wrangler"
      }
    ]
  }
}
